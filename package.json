{
  "name": "MarcelWeb",
  "version": "0.1.0",
  "repository": "https://github.com/MarcelForArt/marcel-web.git",
  "private": true,
  "scripts": {
    "start": "cross-env PORT=${PORT} TARGET_ENV=development API_URL=https://staging.marcelforart.com/api webpack serve --progress --hot --inline --mode development --config=webpack/development.js",
    "env:dev": "cross-env TARGET_ENV=development",
    "env:prod": "cross-env TARGET_ENV=production",
    "env:staging": "cross-env TARGET_ENV=staging",
    "env:qa": "cross-env TARGET_ENV=qa",
    "env:storybook": "cross-env TARGET_ENV=storybook",
    "test": "cucumber-js",
    "test:auto": "jest",
    "storybook": "start-storybook -p 9001 -c storybook",
    "test:local": "cross-env IS_LOCAL=true jest",
    "coverage": "npm test -- --coverage",
    "postcoverage": "opn coverage/lcov-report/index.html",
    "lint:css": "stylelint './src/**/*.js'",
    "build:clean": "rimraf \"dist/!(.git*|Procfile)**\"",
    "build:copy": "copyfiles -u 1 public/* public/**/* dist",
    "prebuild": "npm run build:clean && npm run build:copy",
    "build": "npm run prebuild && cross-env TARGET_ENV=production API_URL=https://app.marcelforart.com/api webpack --mode production --config=webpack/production.js",
    "buildstaging": "npm run prebuild && cross-env TARGET_ENV=staging API_URL=https://staging.marcelforart.com/api webpack --mode production --config=webpack/production.js",
    "builddev": "npm run prebuild && cross-env TARGET_ENV=development API_URL=https://staging.marcelforart.com/api webpack --mode production --config=webpack/production.js",
    "deploy:staging": "yarn build && aws s3 sync dist/ s3://staging-marcel-webapp --exclude 'bundle-analyzer.html' --delete",
    "deploy:production": "yarn build && aws s3 sync dist/ s3://prod-marcel-webapp --exclude 'bundle-analyzer.html' --delete"
  },
